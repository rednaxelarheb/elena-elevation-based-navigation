{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["start_address","length"],
  "properties": {
    "start_address": {
      "type": "object",
      "required": ["latitude", "longitude"],
      "properties": {
        "latitude": {
          "type": "number"
        },
        "longitude": {
          "type": "number"
        }
      }
    },
    "max_or_minimize_change": {
      "type": "boolean"
    },
    "length": {
      "type": "number"
    }
  }
}