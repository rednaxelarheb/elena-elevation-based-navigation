{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["start_address","length"],
  "properties": {
    "start_address": {
      "type": "object",
      "required": ["latitude", "longitude"],
      "properties": {
        "latitude": {
          "type": "number"
        },
        "longitude": {
          "type": "number"
        }
      }
    },
    "desired_uphill": {
      "type": "number"
    },
    "length": {
      "type": "number"
    },
    "desired_progile": {
      "distances": {
        "type": "array",
        "items": {
          "type": "number"
        }
      },
      "altitudes": {
        "type": "array",
        "items": {
          "type": "number"
        }
      } 
    }
  }
}
